import random
numberguess = random.randrange (0,10)
myguess = int(input("what is your guess? 1-10"))

running=""
if myguess > numberguess:
   print ("lower")

if myguess < numberguess:
   print ("higher")

while running != myguess == numberguess:
  if myguess == numberguess:
   print ("you are correct")

